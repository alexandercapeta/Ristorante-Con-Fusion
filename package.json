{
  "name": "ristorantefusion",
  "version": "1.0.0",
  "description": "A webpage for Ristorante Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/font/* dist/fonts",
    "imagemin": "imagemin img/* --out-dir='dist/img'",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandercapeta/Ristorante-Con-Fusion.git"
  },
  "author": "Alexander Rivera",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexandercapeta/Ristorante-Con-Fusion/issues"
  },
  "homepage": "https://github.com/alexandercapeta/Ristorante-Con-Fusion#readme",
  "devDependencies": {
    "lite-server": "^2.5.4",
    "node-sass": "^4.7.2",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.1",
    "rimraf": "^2.6.2",
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.12.9",
    "sass": "^1.26.10"
  }
}
